   "source": [
    "CREATE OR REPLACE VIEW gold.vw_vendas_diarias AS\n",
    "SELECT\n",
    "  dt.data,\n",
    "  SUM(f.revenue_net) AS receita_liquida,\n",
    "  SUM(f.quantity)    AS itens\n",
    "FROM gold.fact_vendas f\n",
    "JOIN gold.dim_tempo dt ON dt.tempo_sk = f.fk_tempo_sk\n",
    "GROUP BY dt.data;\n"
   ]
  }
 ],
